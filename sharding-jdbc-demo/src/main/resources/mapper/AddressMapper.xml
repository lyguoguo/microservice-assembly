<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sharding.mapper.AddressMapper">
    <insert id="save" parameterType="com.example.sharding.entity.Address">
        INSERT INTO t_address(code,name,pid,type,lit) VALUES ( #{code},#{name},#{pid},#{type},#{lit} )
    </insert>

    <select id="get" parameterType="java.lang.Long" resultType="com.example.sharding.entity.Address">
    	select * from t_address where id = #{id}
    </select>
</mapper>
